        "# Crawl Data\n",
        "\n",
        "filename = 'Pilpres.csv'\n",
        "search_keyword = 'prabowo gibran presiden terpilih until:2024-05-14 since:2024-03-20 lang:id'\n",
        "limit = 1500\n",
        "\n",
        "!npx -y tweet-harvest@2.6.0 -o \"{filename}\" -s \"{search_keyword}\" --tab \"LATEST\" -l {limit} --token {twitter_auth_token}"
      ],
